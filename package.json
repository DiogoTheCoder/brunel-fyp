{
	"name": "make-java-great-again",
	"displayName": "Make Java Great Again!",
	"description": "",
	"version": "0.0.1",
	"license": "UNLICENSED",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:make-java-great-again.refactorFile",
		"onCommand:make-java-great-again.displaySyntaxTree"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "make-java-great-again.refactorFile",
				"title": "MJGA: Refactor File"
			},
			{
				"command": "make-java-great-again.displaySyntaxTree",
				"title": "MJGA: Display Simple Abstract Syntax Tree"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/flat": "^5.0.1",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/prettier": "2.1.5",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"deepdash": "^5.3.2",
		"flat": "^5.0.2",
		"java-ast": "^0.3.0",
		"mulang": "^5.3.0",
		"prettier": "2.2.1",
		"prettier-plugin-java": "^1.0.1"
	}
}
