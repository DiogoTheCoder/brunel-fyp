{
  "name": "make-java-great-again",
  "displayName": "Make Java Great Again!",
  "description": "",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "make-java-great-again.refactorFile",
        "category": "MJGA",
        "title": "Refactor File"
      },
      {
        "command": "make-java-great-again.displaySyntaxTree",
        "category": "MJGA",
        "title": "Display Simple Abstract Syntax Tree"
      }
    ],
    "languages": [
      {
        "id": "java",
        "aliases": [
          "Java",
          "java"
        ],
        "extensions": [
          ".java",
          ".class"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "java",
        "scopeName": "source.java",
        "path": "./syntaxes/java.tmLanguage.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Example configuration",
      "properties": {
        "java.maxNumberOfProblems": {
          "scope": "resource",
          "type": "number",
          "default": 100,
          "description": "Controls the maximum number of problems produced by the server."
        },
        "java.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "verbose",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "build": "mvn clean install",
    "lint": "eslint --fix \"**/*.{js,ts,tsx}\"",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/flat": "^5.0.1",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "deepdash": "^5.3.2",
    "flat": "^5.0.2",
    "mulang": "^5.3.0",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "prettier-plugin-java": "^1.0.1",
    "vscode-languageclient": "6.1.3"
  }
}
